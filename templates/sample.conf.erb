input {
  file {
    path => '/var/log/httpd/access_log'
    type => 'apache'
  }
}

filter {
  if [type] == 'apache' {
    grok {
      match => { 'message' => '%{COMBINEDAPACHELOG}' }
    }

    useragent {
      source => 'agent'
    }

    geoip {
      source => 'clientip'
    }
  }
}

output {
  elasticsearch { }
}
